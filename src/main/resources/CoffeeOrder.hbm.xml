<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.esipovich.coffeeshop.model">
    <class name="com.esipovich.coffeeshop.model.CoffeeOrder" table="coffeeorder">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="coffeeKind" column="coffeeKind"/>
        <property name="quantity" column="quantity"/>
        <property name="cost" column="cost"/>
        <property name="deliveryTimeFrom" column="deliveryTimeFrom"/>
        <property name="deliveryTimeTo" column="deliveryTimeTo"/>
    </class>
</hibernate-mapping>